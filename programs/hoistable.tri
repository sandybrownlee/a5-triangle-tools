let var i : Integer in
begin
    i := 10;
    let var a : Integer;
        var b : Integer;
        var c : Integer; in
    begin
        a := 4;
        b := 8;
        c := 10;
        while i > 0 do
            begin
                putint(b + (((a * a) / (b + b)) + c));
                i := i - 1;
                c := a * b;
            end
    end
end
